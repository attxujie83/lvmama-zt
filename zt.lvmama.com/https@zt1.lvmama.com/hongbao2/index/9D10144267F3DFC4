function showDiscountList(str,strJson){
	var j=strJson.length;
	var needStr ="";
	var isUsed = "change";
	if(ljsy=='NEED'){
		needStr = " <button class='use' onclick = 'useNow()'></button>";
		isUsed = "";
	}
	 for(var i=0;i<j;i++){
		 var money =strJson[i].money!=null?strJson[i].money:"";
		 var title =strJson[i].title!=null?strJson[i].title:"";
         var validity =strJson[i].validity!=null?strJson[i].validity:"";
		 str=str+
		"<div class='list-show'>"+ 
		"<div class='quan "+isUsed+"'>"+
		"<div class='price'><span>&yen;"+money+"</span></div>"+
		"<div class='detail'>"+
		"<div class = 'msg'>"+
		"<p class='name'>"+title+"</p>"+
		"<p class='time'>"+validity+"</p>"+
		"</div> </div> </div>"+
		needStr
		+
		"</div></div>" 
		
		
		
		
	 }return str;
}

function showFailMessage(str,message){
	str = str+
    "<p>"+message+"</p>"+
   "<button class='next-time'></button>"
  return str;
}

//点击红包树领取优惠券
$(".tree").click(function(){
	var lvsessionid = global.getCookie("lvsessionid")||uuId;
	if (lvsessionid) {
		$.ajax({
			url:"../hbtreeDiscount@lvsessionid="+lvsessionid+"&thematicId="+thematicId+"&v="+getVersion(),
			type:"GET",
			dataType:"json",
			async:false,
			error:function(){
				global.createDialog("网络不畅通，请刷新重试！",1);
			},
			success:function(data){
				if(data.isLogin){
					      if(data.code==1){
		   				  var str="";
		  	        	  var strJson=data.oActComm;
		  	        	  var type = strJson.back1;

		  	        	  var money =strJson.money!=null?strJson.money:"";
		  	 		      var title =strJson.title!=null?strJson.title:"";
		  	              var validity =strJson.validity!=null?strJson.validity:"";
		  	        	  if("N" == type){
			  	        		 str=str+
				  	   		      "<p class='name'>"+title+"</p>";
				  	   		 
			  	          }else{
		  	        	         str=str+
		  	   		               "<div class='quan'>"+
		  	   		               "<div class='price'><span>&yen;"+money+"</span></div>"+
		  	   		               "<div class='detail'>"+
		  	   		               "<div class = 'msg'>"+
		  	   		               "<p class='name'>"+title+"</p>"+
		  	   		               "<p class='time'>"+validity+"</p>"+
		  	   		               "</div></div> </div>"+
		  	   		               "</div>" ;		  	   		               
		  	   		               
		  	   		               
		  	   		               
			  	        	  }
		  				  $("#winPopup").html(str); 
		  				  $("#getPopup").show();
					      }else if("-1" == data.code){
					    	  if("-3" == data.station || "-14" == data.station ){
					    		  global.createDialog(data.message,1);
					    	  }else{
					    		  $("#fail_popup").show(); 
					    	  }
					    	 
					      }
					
				}else{
					jumpLogin();    
				}
			}
		});
	}else{
		jumpLogin(); 
	}
});

//豆腐块点击领取优惠巻
function getDFKInfo(id){
	var lvsessionid = global.getCookie("lvsessionid")||uuId;
	var hbType = "DFK";
	if (lvsessionid) {
		$.ajax({
			url:"../hbtreeDiscount@lvsessionid="+lvsessionid+"&thematicId="+thematicId+"&oActInfoId="+id+"&hbType="+hbType+"&v="+getVersion(),
			type:"GET",
			dataType:"json",
			async:true,
			error:function(){
				global.createDialog("网络不畅通，请刷新重试！",1);
			},
			success:function(data){
				if(data.isLogin){
					if(data.code==1){
						 if(data.DFK_Y!="Y"){
						  var str="";
		  	        	  var strJson=data.oActComm;
		  	        	  var type = strJson.back1;
		  	        	  var money =strJson.money!=null?strJson.money:"";
		  	 		      var title =strJson.title!=null?strJson.title:"";
		  	              var validity =strJson.validity!=null?strJson.validity:"";
		  	              
		  	             if("N" == type){
		  	        		 str=str+
			  	   		      "<p class='name'>"+title+"</p>";
			  	   		 
		  	             }else{
	  	        	         str=str+
	  	   		               "<div class='quan'>"+
	  	   		               "<div class='price'><span>&yen;"+money+"</span></div>"+
	  	   		               "<div class='detail'>"+
	  	   		               "<div class='msg'>"+
	  	   		               "<p class='name'>"+title+"</p>"+
	  	   		               "<p class='time'>"+validity+"</p>"+
	  	   		               "</div></div> </div>"+
	  	   		               "</div>" ;
		  	        	  }
		  	        	
		  	    
		  	             
		  				  $("#winPopup").html(str); 
		  				  $("#getPopup").show();
						 }else{
							global.createDialog(data.message,1);
						 }
						
					}else if("-3"==data.station || "-14" == data.station){      //已经领取过红包的情况
						/* var str ="";
						str = showFailMessage(str,data.message);
						$("#failPopup").html(str);
						$("#fail_popup").show(); */
						global.createDialog(data.message,1);
					}else{
					   $("#fail_popup").show();
					}
					//将领取的劵码置为已领取样式
					if(data.oActComm!=null && data.oActComm!=""){
						
						//在这判断下是不是豆腐块组合卷  是的话 换一个样式，明天找前端要样式
						var id = data.oActComm.id;
						var type = data.oActComm.back1;
						var code = data.code;
						if("N" == type && code == "-1"){   //组合卷的豆腐块 且未抽中奖
							$("#dfk_"+id).addClass("select join");
						}else{
							$("#dfk_"+id).addClass("select");
						}
		
						
						
					}
					if(data.oActCommList!=null && data.oActCommList!=""){
						console.log(data.oActCommList);
						for(var i = 0 ; i<data.oActCommList.length;i++ ){
							var id = data.oActCommList[i].id;
							$("#dfk_"+id).addClass("select");
						}
					}
				}else{
					jumpLogin();
				}
			}
		});
	}else{
		jumpLogin();
	}
}



 //点击转盘转动抽奖js
$(function(){
	var timeOut = function(message){  //超时函数
		$("#btnDraw").rotate({
			angle:0, 
			duration: 10000, 
			animateTo: 2160, //这里是设置请求超时后返回的角度，所以应该还是回到最原始的位置，2160是因为我要让它转6圈，就是360*6得来的
			callback:function(){
				global.createDialog(message,1);
			}
		}); 
	}; 

	var rotateFunc = function(awards,angle,title,money,validity,type){  //awards:奖项，angle:奖项对应的角度
		$('#btnDraw').stopRotate();
		$("#btnDraw").rotate({
			angle:0, 
			duration: 5000, 
			animateTo: angle+1440, //angle是图片上各奖项对应的角度，1440是我要让指针旋转4圈。所以最后的结束的角度就是这样子^^
			callback:function(){
				      if("" == money || null == money){
				    	  money="";
				      }else{
				    	  money ="&yen;"+money;
				      }
					  var str="";
				      if("N" == type){      //非优惠巻类型
		  	        		 str=str+
			  	   		      "<p class='name'>"+title+"</p>";
		  	          }else{
		  	        	 str=str+
		  	   		      "<div class='quan'>"+
		  	   		      "<div class='price'><span>"+money+"</span></div>"+
		  	   		      "<div class='detail'>"+
		  	   		      "<div class='msg'>"+
		  	   		      "<p class='name'>"+title+"</p>"+
		  	   		      "<p class='time'>"+validity+"</p>"+
		  	   		      "</div></div> </div>"+
		  	   		      "</div>" ;
		  	        	
		  	          }
				      $("#winPopup").html(str); 
	  				  $("#getPopup").show();
				     
				
				$("#btnAlready").show();
				$("#btnDraw").hide();
				
			}
		}); 
	};

	$("#btnDraw").rotate({ 
	   bind: 
		 { 
			click: function(){
				var time = [0,1];
				var code;
				var zpshow;
				var lvsessionid = global.getCookie("lvsessionid")||uuId;
				$.ajax({
					url:"../hbtreeDiscount@lvsessionid="+lvsessionid+"&thematicId="+thematicId+"&v="+getVersion(),
					type:"GET",
					dataType:"json",
					async:false,
					error:function(){
						time =0;
					},
					success:function(data){
						if(data.isLogin){
							time = 1;
							angle =data.angle;
	                        code = data.code;
	                        station = data.station;
	                        zpshow = data.oActComm;
							if(code=="-1"){
								time = 2;
								if("-3" == station || "-14" == station){
									global.createDialog(data.message,1);
								}else{
									$("#fail_popup").show();
								}
								
								$("#btnAlready").show();
								$("#btnDraw").hide();
							}
							
							
						}else{
							jumpLogin();
						}
					}
				});
				 if(time==0){
					timeOut("网络不通畅，请稍后再试试！"); //网络超时
				}
				if(time==1){
	
					if(angle==0){
						rotateFunc(0,0,zpshow.title,zpshow.money,zpshow.validity,zpshow.back1)
					}
					if(angle==1){
						rotateFunc(1,60,zpshow.title,zpshow.money,zpshow.validity,zpshow.back1)

					}
					if(angle==2){
						rotateFunc(2,120,zpshow.title,zpshow.money,zpshow.validity,zpshow.back1)

					}
					if(angle==3){

						rotateFunc(3,180,zpshow.title,zpshow.money,zpshow.validity,zpshow.back1)
		
					}
					if(angle==4){

						rotateFunc(4,240,zpshow.title,zpshow.money,zpshow.validity,zpshow.back1)
		
					}
					if(angle==5){

						rotateFunc(5,300,zpshow.title,zpshow.money,zpshow.validity,zpshow.back1)
						
					}if(angle =='undefined'){
						time = 2;
						
						$("#fail_popup").show();
						$("#btnAlready").show();
						$("#btnDraw").hide();
					}
				} 
			}
		 } 
	   
	});
	
})

function getVersion(){
	var d = new Date();
	return d.getTime();
}

var lvsessionid = global.getCookie("lvsessionid")||uuId;
var callback=function(data){
    if(data.code==-3){
        console.log("未登陆,跳登陆台");
        loginUtil.gotoLogin({callbackUrl:'', successCbName:'success', failCbName:'fail'})
    }else if(data.code==1){
        //登陆验证通过后的处理
        console.log("已登陆");
        success(data.data);
    }
}

function jumpLogin(){
	loginUtil.gotoLogin({callbackUrl:'', successCbName:'success', failCbName:'fail'});
}
function success(data) {
	location.reload();
}
function fail(){
}
 
function uuid() {
    var s = [];
    var hexDigits = "0123456789abcdef";
    for (var i = 0; i < 36; i++) {
        s[i] = hexDigits.substr(Math.floor(Math.random() * 0x10), 1);
    }
    s[14] = "4";  // bits 12-15 of the time_hi_and_version field to 0010
    s[19] = hexDigits.substr((s[19] & 0x3) | 0x8, 1);  // bits 6-7 of the clock_seq_hi_and_reserved to 01
    s[8] = s[13] = s[18] = s[23] = "-";
 
    var uuid = s.join("");
    return uuid;
}
