<script type="text/javascript" src="../../../https@pics.lvjs.com.cn/minify/@@static_2Fzt_2Fpublic_2Fjs_2Fbase_2FnewBase.js,static_2Fplugins_2FnativeJs_2Flogin_2F1.0_2Fbuild.min.js,s06580ED77F"></script>
<script type="text/javascript">
var proshow='IMGLIST';
var shownum='FIVE';
var ljsy = 'NEED';   //是否立即使用
var thematicId='9D10144267F3DFC4';
var sendHBWay = 'DFK';   //发送红包的方式
var uuId = uuid();
changeImg();
if(sendHBWay=="DFK"){
	  var lvsessionid = global.getCookie("lvsessionid")||uuId;
	  if(lvsessionid){  
		  $.ajax({
				url:"../getMyDiscount@lvsessionid="+lvsessionid+"&thematicId="+thematicId+"&v="+getVersion(),
				type:"GET",
				dataType:"json",
				async:false,
				error:function(){
				},
				success:function(data){
				 if(data.isLogin){
				    var str="";
		        	var strJson=data.disInfolist;
		        	if(strJson.length>0){
		        		for(var i = 0 ; i<strJson.length;i++ ){
							var id = strJson[i].id;
							$("#dfk_"+id).addClass("select");
						}
		        	}
				}
				}
			});   
	    }
}

$("article").each(function(i){
	var host=window.location.href;
	var tabId = $(this).attr("data-tabid");
	var prourl=(host.indexOf("v=")==-1)?'../product/'+tabId+"@proshow="+proshow:'../product/'+tabId+"@proshow="+proshow+'&v='+global.getUrlParam("v");
	var $this=$(this);
	$.ajax({
		url:prourl,
		type:"GET",
		dataType:'html',
		async:true,
 		error:function(){
			global.createDialog("网络不畅通，请刷新重试！",1);
		},  
		success:function(data){
			$this.append(data);
			$this.find(".a").css("display","none");
			//产品个数超过设置显示的个数隐藏
			$this.find(".prd-list").each(function(j){
				var probj = $(this);
				if(shownum=="TWO"){
					if(j>1){
						probj.css("display","none");
					}
				}else if(shownum=="THREE"){
					if(j>2){
						probj.css("display","none");
					}
				}else if(shownum=="FOUR"){
					if(j>3){
						probj.css("display","none");
					}
				}else if(shownum=="FIVE"){
					if(j>4){
						probj.css("display","none");
					}
				}
			});
		}
	});
});
 
//获取验证码
function getMobileAuthCode(){
	var id =thematicId;
	var mobile = $("#mobile").val();
	var imgCode = $("#msgCode").val();// 图片验证码 
	var lvsessionid = global.getCookie("lvsessionid")||uuId;
	if(lvsessionid){   
		$.ajax({
            type: "GET", 
            url: "../../hongbao/getMobileAuthCode/"+id+"@mobile="+mobile+"&imgCode="+imgCode+"&lvsessionid="+lvsessionid+"&v="+getVersion(),
            dataType: "json",
            async: false,
            error:function(){
            },
            success: function(data) {
                if(1 != data.code){
                	if(null == data.message){
                		global.createDialog('系统忙，请稍后!',1);
                	} else {
                		global.createDialog(data.message,1);
                		if(data.station="-10"){
                			changeImg();
                		}
                	}
                } else {
                	 // 倒计时  moibleAuthCode
                	 var timeCount = 60;
                	 $('#moibleAuthCode').hide();
                	 $('#moibleDaojishi').show();
                	 var timer = setInterval(function () {
                	        if (timeCount > 0) {
                	        	timeCount--;
                	        	$('#moibleDaojishi').html('剩余' +timeCount+'秒');
                	        } else {
                            	 $('#moibleDaojishi').hide();
                	        	 $('#moibleAuthCode').show();
                	             clearInterval(timer);
                	        }
                	    }, 1000);
                }
            }
      });
	}else{
		global.createDialog("网络不畅通，请刷新重试！",1);
	}
}

//校验手机号、图形验证码
function checkMobile(mobile,imgCode){
	var telTrue=/^(13[0-9]|14[0-9]|15[0-9]|17[0-9]|18[0-9])\d{8}$/i.test(mobile);
	if(mobile=="" || !telTrue){
		global.createDialog('请填写正确的手机号码',1);
		return false;
	}
	if(imgCode==""){
		global.createDialog('图形验证码不能为空',1);
		return false; 
	}
	return true; 
}
//检验内容不能为空
function checkMsgCode(msgCode,msg){
	if(!msgCode){
		global.createDialog(msg,1);
		return false; 
	}
	return true;
}

// 点击一种品类的立即领取按钮
var clickFlg = false;
$("#lingquBtn").click(function(){
	var obj = $(this);
	var id =thematicId;
	var mobile = $("#mobile").val();
	var convertCode = $("#convert").val(); //兑换码
	var imgCode = $(".short").val();// 图片验证码 
	var msgCode = $("#authCode").val();// 短信验证码
	var lvsessionid = global.getCookie("lvsessionid")||uuId;
	if(lvsessionid && checkMobile(mobile,imgCode) && checkMsgCode(msgCode,"短信验证码不能为空") && !clickFlg){ 
		clickFlg = true;
		$.ajax({
	            type: "GET", 
	            url: "../regAndLogin/"+id+"@mobile="+mobile+"&convertCode="+convertCode+"&msgCode="+msgCode+"&imgCode="+imgCode+"&lvsessionid="+lvsessionid+"&v="+getVersion(),
	            dataType: "json",
	            error:function(data){
	            	global.createDialog("网络不畅通，请刷新重试",1);
	            },
	            success: function(data) {
	   				if(data.code=="1"){
	   				  var str="";
	  	        	  var strJson=data.oActComm;
	  	        	  for(var i=0;i<strJson.length;i++){
		  	        	  var money = strJson[i].money!= null ? strJson[i].money :'';
		  	        	  var title = strJson[i].title !=null ? strJson[i].title : '';
		  	        	  var validity = strJson[i].validity != null ? strJson[i].validity :'';
		  	        	  str=str+
		  	   		      "<div class='quan'>"+
		  	   		      "<div class='price'><span>&yen;"+money+"</span></div>"+
		  	   		      "<div class='detail'>"+
		  	   		      "<div class = 'msg'>"+
		  	   		      "<p class='name'>"+title+"</p>"+
		  	   		      "<p class='time'>"+validity+"</p>"+
		  	   		      "</div></div></div>"+
		  	   		      "</div>" ;
	  	        	  }
	  				  $("#winPopup").html(str); 
	  				  $("#getPopup").show();
	   				}else{
	   				  if("-1" == data.code){
	   					if(null == data.message){
	                		global.createDialog('系统忙，请稍后!',1);
	                	} else {
	   						global.createDialog(data.message,1);
	                	}
	   				  }
	   				}
	   				clickFlg = false;
	            }
	      });
	}
});
function changeImg(){ 
    var lvsessionid = global.getCookie("lvsessionid")||uuId;
	$("#authCodeChange").attr("src","../../../https@m.lvmama.com/usso/router/rest.do@method=api.com.validateCode.createNewValidateCode&version=1.0.0&lvversion=7.9.5&validateTemplateId=21&lvsessionid="+lvsessionid); 
}

 // 点击我的优惠券
$("#myCoupons").click(function(){
  $(".no-quan").hide();
  $(".list-show").html(""); 
  var lvsessionid = global.getCookie("lvsessionid")||uuId;
  if(lvsessionid){  
	  $.ajax({
			url:"../getMyDiscount@lvsessionid="+lvsessionid+"&thematicId="+thematicId+"&v="+getVersion(),
			type:"GET",
			dataType:"json",
			async:false,
			error:function(){
				global.createDialog("网络不畅通，请刷新重试！",1);
			},
			success:function(data){
			 if(data.isLogin){
			    var str="";
	        	var strJson=data.disInfolist;
	        	if(strJson.length>0){
	        		str=showDiscountList(str,strJson);  
					$("#myCoupon").html(str);
					if(strJson.length>3){
						$("#myCoupon").removeClass().addClass("my-coupon many"); 	
					}
	        	}else{
	        		$(".no-quan").show();	
	        	}
	        	$("#myCoupon").css("display","block");				
	        	var mcTop = $("#myCoupon").outerHeight()
	        	$(".coupon-logo span").css("top",mcTop);
			}else{
				jumpLogin();
			 }
			}
		}); 
    }else{
		jumpLogin();
	}
});
 function useNow(){
	 var ljsyUrl = '';
	 if(ljsyUrl!=null && ljsyUrl!=""){
		 window.location.href = ljsyUrl;
	 }else{
		 $("#getPopup").hide();
		 $("#myCoupon").css("display","none");
		 $(".coupon-logo span").css("top","2rem");
		 $("body").animate({scrollTop:$(".prd-show").offset().top-20},500); 
	 }
 }
 
