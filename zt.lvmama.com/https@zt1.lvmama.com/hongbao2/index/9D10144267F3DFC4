		$(".no-quan").hide();
		 $("#myCoupon").css("display","none");
		 $(".coupon-logo span").css("top","2rem");
 }); 
 $("#lahuan").click(function(){
	 var a = document.getElementById("myCoupon").style.display;
	 if("block" == a){
	  $("#myCoupon").css("display","none");
	  $(".coupon-logo span").css("top","2rem");
	  return false;
	 } 
});

$('body div').bind('click', function(event) {
    // IE支持 event.srcElement ， FF支持 event.target    
    var evt = event.srcElement ? event.srcElement : event.target;    
    if(evt.id == 'myCoupons' || evt.id == 'lahuan') return; // 如果是点击我的优惠券或拉环不操作
    else {
    	$("#myCoupon").hide();
    	$(".coupon-logo span").css("top","2rem");
    }   
});

